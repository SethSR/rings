
-- 1MB = 1_048_576
region wram_low [1_048_576] @ 0x0600_0000;
region wram_high[1_048_576] @ 0x0600_4000;

record vec2 {
	x: s32,
	y: s32,
}

table positions[200] {
	pos: vec2,
}

table enemies[50] {
	pos_idx: u8, -- positions index
	hp: u8,
}

table players_alive[4] {
	pos_idx: u8, -- positions index
	hp: u8,
}

table players_dead[4] {
	pos_idx: u8, -- positions index
}

main {
	enemy_id : u8 = 0;
	-- loop-begin
	enemy_id += 1;
	-- loop-end
}

